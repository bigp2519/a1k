{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "types": ["three"]
  },
  "include": [
  "next-env.d.ts",
  "**/*.ts",
  "**/*.tsx",
  "skip-go.d.ts",
  "types/**/*.d.ts"
],
  "exclude": ["node_modules"]
}
